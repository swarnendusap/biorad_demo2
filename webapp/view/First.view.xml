<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" controllerName="biorad.demo.controller.First">
	<Page title="First View Page">
		<!--showNavButton="true" navButtonPress="onNavBack"-->
		<Panel headerText="Panel: Addition Example">
			<content>
				<Input id="input1" width='10%'></Input><Text text=" + "/>
				<Input id="input2" width='10%'></Input>
				<Button text="=" press="onAlertButtonPress"/>
				<Input id="input3" width='15%'></Input>
				<List headerText="Invoices" class="sapUiResponsiveMargin" width="auto" items="{invoiceL>/Invoices}">
					<items>
						<ObjectListItem title="{invoiceL>Quantity} x {invoiceL>ProductName}"/>
					</items>
				</List>
			</content>
		</Panel>
		<Button id="nextBtn" text="Move to Airline Page" press="onNextBtnPrs"/>
	</Page>
</mvc:View>